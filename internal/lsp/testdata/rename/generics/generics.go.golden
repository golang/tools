// +build go1.18

package generics

type H[P any] struct {
	F int
}

func (H[_]) M() {}

func F[P any](P) {
	var p P //@rename("P", "Q")
	_ = p
}

func _() {
	var x H[int] //@rename("G", "H")
	_ = x.F      //@rename("F", "K")
	x.M()        //@rename("M", "N")

	var y H[string]
	_ = y.F
	y.M()
}

